<%= alchemy_form_for [:admin, @essence_video] do |f| %>
  <%= f.input :width %>
  <%= f.input :height %>
  <%= f.input :autoplay %>
  <%= f.input :controls %>
  <%= f.input :loop %>
  <%= f.input :muted %>
  <%= f.input :preload, collection: %w(none metadata auto), include_blank: true %>
  <%= f.submit Alchemy.t(:save) %>
<% end %>
